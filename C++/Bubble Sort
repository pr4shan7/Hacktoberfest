#include<iostream>
using namespace std;
int main()
{
  int n, i, j, t;
  cout << "How Many Numbers are to be sorted:\n";
  cin >> n;
  int arr[n];
  cout << "Enter " << n << " Numbers Which are to be Sorted:\n";
  for (i = 0; i <= n - 1; i++)
    {
      cin >> arr[i];
    }
  for (i = 1; i < n; i++)
    {
      for (j = 0; j < n - i; j++)
	{
	  if (arr[j] > arr[j + 1])
	    {
	      t = arr[j];
	      arr[j] = arr[j + 1];
	      arr[j + 1] = t;
	    }
	}
    }
  cout << "\n Sorted Array In Ascending Order is:\n";
  for (i = 0; i <= n - 1; i++)
    {
      cout << arr[i] << " ";
    }

}
